{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "image_url",
        "body",
        "posted_at",
        "posted_by"
    ],
    "properties": {
        "image_url": {
            "type": "string",
            "default": "",
            "title": "The image_url Schema",
            "examples": [
                "ssdfsdsdf.com"
            ]
        },
        "body": {
            "type": "string",
            "default": "",
            "title": "The body Schema",
            "examples": [
                "shfkjshfkjhsdkjfhsdjkfhjkshdfkjshdf"
            ]
        },
        "posted_at": {
            "type": "string",
            "default": 0,
            "title": "The posted_at Schema",
            "examples": [
                "11-11-2022"
            ]
        },
        "posted_by": {
            "type": "string",
            "default": "",
            "title": "The posted_by Schema",
            "examples": [
                "username"
            ]
        }
    },
    "examples": [
        {
            "image_url": "ssdfsdsdf.com",
            "body": "shfkjshfkjhsdkjfhsdjkfhjkshdfkjshdf",
            "posted_at": "11-26-2022",
            "posted_by": "username"
        }
    ]
}